# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.ts]
indent_style = tab

[{package.json,.travis.yml}]
indent_size = 2
indent_style = space
trim_trailing_whitespace = true
